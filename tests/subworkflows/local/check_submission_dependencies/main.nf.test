                { assert workflow.out.submitted_analysis_channels.clinical.experiment},
                { assert workflow.out.submitted_analysis_channels.files},
                { assert workflow.out.validated_analysis_channels},
                { assert workflow.out.validated_analysis_channels.meta},
                { assert workflow.out.validated_analysis_channels.analysis},
                { assert workflow.out.validated_analysis_channels.analysis.files},
                { assert workflow.out.validated_analysis_channels.analysis.workflow},
                { assert workflow.out.validated_analysis_channels.analysis.analysis},
                { assert workflow.out.validated_analysis_channels.clinical},
                { assert workflow.out.validated_analysis_channels.clinical.sample},
                { assert workflow.out.validated_analysis_channels.clinical.specimen},
