    test("check check_submission_dependencies happy path") {
        when {
            workflow {
                """
                params.clinical_url = "https://submission.pcgl-dev.cumulus.genomeinformatics.org"
                params.file_manager_url = "https://file-manager.pcgl-dev.cumulus.genomeinformatics.org"
                // Test input with molecular metadata files (all TSV format)
                input[0] = "EXAMPLE-CA9"
                input[1] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Files.tsv')]
                input[2] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Analysis.tsv')]
                input[3] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Workflow.tsv')]
                input[4] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Read_Group.tsv')]
                input[5] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Experiment.tsv')]
                input[6] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Specimen.tsv')]
                input[7] = [file('${projectDir}/tests/subworkflows/local/check_submission_dependencies/test_data/happy_path/Sample.tsv')]
                input[8] = ['${projectDir}/tests/modules/local/check_dependencies/test_data/happy_path']
                """
            }
        }

        then {
            assertAll(
                { assert workflow.out.versions},
                { assert workflow.out.status_files},
                { assert workflow.out.analysis_channels},
                { assert workflow.out.analysis_channels.meta},
                { assert workflow.out.analysis_channels.analysis},
                { assert workflow.out.analysis_channels.analysis.files},
                { assert workflow.out.analysis_channels.analysis.workflow},
                { assert workflow.out.analysis_channels.analysis.analysis},
                { assert workflow.out.analysis_channels.clinical},
                { assert workflow.out.analysis_channels.clinical.sample},
                { assert workflow.out.analysis_channels.clinical.specimen},
                { assert workflow.out.analysis_channels.clinical.read_group},
                { assert workflow.out.analysis_channels.clinical.experiment},
                { assert workflow.out.analysis_channels.files},
                { assert workflow.out.submitted_analysis_channels},
                { assert workflow.out.submitted_analysis_channels.meta},
                { assert workflow.out.submitted_analysis_channels.analysis},
